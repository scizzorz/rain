import iter

var double = func(n) -> n * 2

var main = func()
  var a = [10, 11, 12, 13]

  print("keys from a:")
  for x in a:keys()
    print(x)

  print("values from a:")
  for x in a:values()
    print(x)

  print("doubled values from a:")
  for x in iter.map(a:values(), double)
    print(x)

  var b = a:map(double)

  print("key/values from b:")
  for [k, v] in b:items()
    print(tostr(k) $ " = " $ tostr(v))
